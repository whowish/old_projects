/*==================================================================================================================================
==== Background Color
==================================================================================================================================*/
 .blueToBlack_bg{
	 background: #3b6ca6; /* for non-css3 browsers */
	 /* For Internet Explorer 5.5 - 7 */
	 filter: progid:DXImageTransform.Microsoft.gradient(GradientType=1, startColorstr=#FF3B6CA6, endColorstr=#FF202020);
	 /* For Internet Explorer 8 */
	 -ms-filter: progid:DXImageTransform.Microsoft.gradient(GradientType=1, startColorstr=#FF3B6CA6, endColorstr=#FF202020);
	 
	 background: -webkit-gradient(linear, left top, right top, from(#3b6ca6), to(#202020)); /* for webkit browsers */
	 
	 background: -moz-linear-gradient(left,  #3b6ca6,  #202020); /* for firefox 3.6+ */
 }

.yellowToDarkyellow_bg{
	background-color: #F7EC24;
	/* For WebKit (Safari, Google Chrome etc) */
	background: -webkit-gradient(linear, left top, left bottom, from(#F7EC24), to(#dcd21c));
	/* For Mozilla/Gecko (Firefox etc) */
	background: -moz-linear-gradient(top, #F7EC24, #dcd21c);
	/* For Internet Explorer 5.5 - 7 */
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#FFF7EC24, endColorstr=#FFDCD21C);
	/* For Internet Explorer 8 */
	-ms-filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#FFF7EC24, endColorstr=#FFDCD21C);
 }
 
