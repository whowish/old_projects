/*==================================================================================================================================
==== Site Style 
==================================================================================================================================*/
.filter ul li {
	display: block;
	height: 24px;
	padding-right: 8px;
	padding-left: 8px;
	float: left;
}
.filterFeed ul li {
	display: block;
	margin-right: 8px;
	margin-left: 8px;
	float: left;
}
.filter .filter_selected {
	text-decoration: none;
	line-height: 24px;
	background-color: #333333;
	display: block;
}
.filter .filter_selected a {
	text-decoration: none;
	line-height: 24px;
	color: #FFF;
	display: block;
}
.filter ul li a{
	color:#333333;
}
.facebook_pic {
	background-image: url(../images/thumbnail.png);
	background-repeat: no-repeat;
	display: block;
	float: left;
	height: 48px;
	width: 48px;
	margin:1px;
}
.input_type{
	background-color: #E56284;
	color: #333;
	font-size: 11px;
	font-weight: 300;
	text-decoration: none;
	float: left;
	border: 1px solid #39679E;
	height: 18px;
}
.input_type_area{
	background-color: #FFF;
	color: #333;
	font-size: 11px;
	font-weight: 300;
	text-decoration: none;
	float: left;
	border: 1px solid #39679E;
	height: 60px;
}
.input_type_on_pink{
	background-color: #E56284;
	color: #fff;
	font-size: 16px;
	font-weight: bold;
	text-decoration: none;
	float: left;
	border: 1px solid #FFF;
	height: 18px;
}

.bubble_box{
	position:relative;
	white-space: nowrap;
	display:inline-block;
}
.bubble_box:before {
	content:"";
	display:block; /* reduce the damage in FF3.0 */
	position:absolute;
	bottom:-20px; /* value = - border-top-width - border-bottom-width */
	left:22px; /* controls horizontal position */
	width:0;
	height:0;
	border:10px solid transparent;
	border-top-color:#d9e6f3;
}
.upload_pic_button{
	background-image:url(../images/uploadBtn.png);
	display:block;
	height:24px;
	width:126px;
}
.edit_icon{
	background-image: url(../images/icon.png);
	background-repeat: no-repeat;
	background-position: -0px -120px;
	height: 20px;
	width: 20px;
	display:inline-block;
	float:left;
}
.edit_icon:hover{
	background-image: url(../images/icon.png);
	background-repeat: no-repeat;
	background-position: -20px -120px;
	height: 20px;
	width: 20px;
	display:inline-block;
}
.total_page{
	height:20px;
	font-weight:bold;
	font-size:12px;
	color:#FFFFFF;
	display:inline-block;
	line-height:20px;
	padding-left:8px;
	padding-right:8px;
	float:left;
}
.page_number{
	color:#FFFFFF;
}
.page_number .active{
	background-color: #65a1e0;
}
.page_number li{
	display: block;
	float: left;
	height: 20px !important;
	width: 20px;
	margin-left: 4px;
	line-height: 20px;
	text-align: center;
	background-color: #3b6ca6;
}
.page_number li a:hover{
	background-color: #e56284;
	color:#FFF;
}
.page_number li a{
	display: block;
	height: 20px !important;
	width: 20px;
	line-height: 20px;
	background-color: #96C9EE;
	font-weight: bold;
	color: #FFF;
	text-decoration: none;
}
.x_icon{
	background-image: url(../images/icon.png);
	background-repeat: no-repeat;
	background-position: -40px -120px;
	height: 20px;
	width: 20px;
	display:block;
	float:left;
}
.x_icon:hover{
	background-color:#666666;
}
.x_icon:active{
	background-color:#666666;
	background-image: url(../images/icon.png);
	background-repeat: no-repeat;
	background-position: -60px -120px;
}
.round_corner-4{
	-webkit-border-radius: 4px;
	-khtml-border-radius: 4px;	
	-moz-border-radius: 4px;
	border-radius: 4px;
}
.arrow_down{
	content:"";
	background-position:-10px -50px;
	background-image: url(../images/icon.png);
	display: block;
	float: left;
	height: 5px;
	width: 10px;
	background-repeat: no-repeat;
}
.facebook_id_pic{
	content:"Squeks";
	float:left;
	width:50px;
	height:50px;
	background-image:url(../images/fb.jpeg);
	display:block;
}
.stat_icon{
	content:"Squeks";
	background-image:url(../images/icon.png);
	width:20px;
	height:20px;
	display:block;
	background-position: 0px -140px;
	float:left;
}
.stat_icon:hover{
	background-position: 0px -160px;
}
.flag_icon{
	content:"Squeks";
	background-image:url(../images/icon.png);
	width:20px;
	height:20px;
	display:block;
	background-position: -20px -140px;
	float:left;
}
.flag_icon:hover{
	background-position: -20px -160px;
}
.comment_icon{
	background-image: url(../images/icon.png);
	background-repeat: no-repeat;
	background-position: -80px -120px;
	height: 20px;
	width: 20px;
	display:block;
	float:left;
}
.comment_input{
	height: 38px;
	border: 1px solid #54A8E3;
	background-color: #F2F2F2;
	font-size: 11px;
	color: #444444;
	text-decoration: none;
	width: 572px;
	float: left;
}
.comment_button{
	font-size: 14px;
	font-weight: bold;
	color: #FFF;
	background-color: #54A8E3;
	text-align: center;
	display: block;
	height: 40px;
	width: 100px;
	text-shadow:1px 1px #113955;
	float: left;
	line-height: 40px;
}
.comment_button:hover{
	background-color: #B2D5EE;
	text-decoration:none;
}
.none-text-decoration{
	text-decoration:none !important;
}
.textarea_input{
	height: 60px;
	border: 1px solid #54A8E3;
	background-color: #FFF;
	font-size: 11px;
	color: #444444;
	text-decoration: none;
	float: left;
}
.ballon{
	position:absolute;
	z-index: 7999;
}
.ballon_arrow{
	content:"";
	width:20px;
	height:10px;
	background-image:url(../images/icon.png);
	display:block;
	background-position:-20px -50px;
	position: relative;
}
.ballon_content{
	display: inline-block;
	background-color: #E52D35;
	-webkit-border-radius: 6px;
	-khtml-border-radius: 6px;
	-moz-border-radius: 6px;
	border-radius: 6px;
