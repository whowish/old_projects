<li>
	<span class="span-2">
		<input name="" id="chkbox_figure_<%=figure.id%>" class="margin-top-1" type="checkbox" value="<%=figure.id%>" />
	</span>
	<span class="span-8">
		<%=semantic_time(figure.created_date)%>
	</span>
	<span class="span-17">
		<a href="<%=figure.get_url%>">
			<img class="figure_img" alt="" src="<%=figure.get_160x160_thumbnail_image_url%>"  />
		</a>
	</span>
	<span class="span-24">
		<a href="<%=figure.get_url%>">
			<%=figure.title%>
		</a>
	</span>
	<span class="span-25">
		<%creator = Member.first(:conditions=>{:facebook_id=>figure.creator_facebook_id})%>
		
			<%if creator%>
				<a href="/profile?user_id=<%=figure.creator_facebook_id%>" class="dark_blue"><%=creator.name%></a>
			<%end%>
		
	</span>
	<span class="span-12 last_child">
		<button class="gray_button" onclick="submit_approve(this,<%=figure.id%>,'approve');">
			<img src="../images/check-icons.png"  />
		</button>
		<button class="gray_button" onclick="$.whowish_box({url:'/admin_approve_figure/figure_disapprove_reason?id=<%=figure.id%>',background_color:'#54a8e3'});return false;">
		<img src="../images/cross-icons.png"  />
		</button>
		
		<span id="result_<%=figure.id%>">
		</span>
	</span>
</li>
