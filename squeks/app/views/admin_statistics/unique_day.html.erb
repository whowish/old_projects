<span class="span-99 margin-top-1">
	<%=render :partial=>"admin_statistics/menu"%>
	<%
		date = Time.utc(Time.now.utc.year,Time.now.utc.month,Time.now.utc.mday)

		if params[:date] 
			day,month,year = params[:date].split(" ")
			month_hash = {:jan=>1,:feb=>2,:mar=>3,:apr=>4,:may=>5,:jun=>6,:jul=>7,:aug=>8,:sep=>9,:oct=>10,:nov=>11,:dec=>12}
			date = Time.utc(year,month_hash[month.downcase.to_sym],day)
		end
	%>
	<%=render :partial=>"web_metrics/unique_day",:locals=>{:date=>date}%>
</span>